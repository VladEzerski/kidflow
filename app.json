{
  "expo": {
    "name": "KidFlow",
    "slug": "kidflow",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "src/assets/images/icon.png",
    "scheme": "kidflow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vladez.kidflow"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "src/assets/images/android-icon-foreground.png",
        "backgroundImage": "src/assets/images/android-icon-background.png",
        "monochromeImage": "src/assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.vladez.kidflow"
    },
    "web": {
      "output": "static",
      "favicon": "src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "10cf5ca8-076d-449e-92df-b8df4877ca1c"
      }
    },
    "owner": "vlad_ez",
    "updates": {
      "url": "https://u.expo.dev/10cf5ca8-076d-449e-92df-b8df4877ca1c"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
